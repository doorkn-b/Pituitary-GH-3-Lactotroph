<Lems xmlns="http://www.neuroml.org/lems/0.7.3"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.neuroml.org/lems/0.7.3 LEMS_v0.7.3.xsd">

  
    <ComponentType name="pgh3Cell">
          
        <Parameter name="gca" dimension="none"/>
        <Parameter name="gkca" dimension="none"/>
        <Parameter name="gk" dimension="none"/>
        <Parameter name="gir" dimension="none"/>
        <Parameter name="vca" dimension="none"/>
        <Parameter name="vk" dimension="none"/>
        <Parameter name="vir" dimension="none"/>
        <Parameter name="cm" dimension="none"/>
        <Parameter name="taun" dimension="none"/>
        <Parameter name="alpha" dimension="none"/>
        <Parameter name="fcyt" dimension="none"/>
        <Parameter name="kpmca" dimension="none"/>
        <Parameter name="kd" dimension="none"/>
        <Parameter name="vn" dimension="none"/>
        <Parameter name="vm" dimension="none"/>
        <Parameter name="sn" dimension="none"/>
        <Parameter name="sm" dimension="none"/>
        <Parameter name="kserca" dimension="none"/>
        <Parameter name="dact" dimension="none"/>
        <Parameter name="dinact" dimension="none"/>
        <Parameter name="fer" dimension="none"/>
        <Parameter name="pleak" dimension="none"/>
        <Parameter name="dip3" dimension="none"/>
        <Parameter name="vcytver" dimension="none"/>
        <Parameter name="ip3" dimension="none"/>
        <Parameter name="sa" dimension="none"/>
        <Parameter name="r" dimension="none"/>
        <Parameter name="taua" dimension="none"/>
        <Parameter name="tstim" dimension="none"/>

        <Exposure name="v" dimension="none"/>
        <Exposure name="c" dimension="none"/>
        <Exposure name="n" dimension="none"/>
        <Exposure name="cer" dimension="none"/>
        <Exposure name="a" dimension="none"/>
        <Exposure name="nIR" dimension="none"/>
        <Exposure name="rIR" dimension="none"/>

    <Exposure name="x" dimension="none"/>
          
           <Dynamics>
        <StateVariable name="v" dimension="none"/>
        <StateVariable name="c" dimension="none"/>
        <StateVariable name="n" dimension="none"/>
        <StateVariable name="cer" dimension="none"/>
        <StateVariable name="a" dimension="none"/>
        <StateVariable name="nIR" dimension="none"/>
        <StateVariable name="rIR" dimension="none"/>

        <DerivedVariable name="alphaIRn(v)" dimension="none" value="0.09/(1+exp(0.11*(v+100)))"/>
        <DerivedVariable name="betaIRn(v)" dimension="none" value="0.00035*exp(0.07*(v+25))"/>
        <DerivedVariable name="nIRinf(v)" dimension="none" value="1/(1+betaIRn(v)/alphaIRn(v))"/>
        <DerivedVariable name="tauIRn(v)" dimension="none" value="1/(alphaIRn(v) + betaIRn(v))"/>
        
        <DerivedVariable name="alphaIRr(v)" dimension="none" value="30/(1+exp(0.04*(v+230)))"/>
        <DerivedVariable name="betaIRr(v)" dimension="none" value="0.15/(1+exp(-0.05*(v+120)))"/>
        <DerivedVariable name="rIRinf(v)" dimension="none" value="1/(1+betaIRr(v)/alphaIRr(v))"/>
        <DerivedVariable name="tauIRr(v)" dimension="none" value="1/(alphaIRr(v) + betaIRr(v))"/>

        <TimeDerivative variable="nIR" value="(nIRinf(v) - nIR)/tauIRn(v)"/>
        <TimeDerivative variable="rIR" value="(rIRinf(v) - rIR)/tauIRr(v)"/>

        <DerivedVariable name="ica(v)" dimension="none" value="gca*minf(v)*(v-vca)"/>
        <DerivedVariable name="ik(v)" dimension="none" value="gk*n*(v-vk)"/>
        <DerivedVariable name="ikca(v)" dimension="none" value="gkca*w*(v-vk)"/>
        <DerivedVariable name="ikatp(v)" dimension="none" value="gkatp*a*(v-vk)"/>
        <DerivedVariable name="iir(v)" dimension="none" value="gir*nIR*rIR*(v-vir)"/>

        <DerivedVariable name="minf(v)" dimension="none" value="1.0/(1.0+exp((vm-v)/sm))"/>
        <DerivedVariable name="ninf(v)" dimension="none" value="1.0/(1.0+exp((vn-v)/sn))"/>
        <DerivedVariable name="ainf(c)" dimension="none" value="1.0/(1.0+exp((r-c)/sa))">

        <DerivedVariable name="w" dimension="none" value="c^5/(c^5+kd^5)">
        <DerivedVariable name="jmem" dimension="none" value="-(alpha*Ica(v)+kpmca*c)">
        <DerivedVariable name="jserca" dimension="none" value="kserca*c">

        <DerivedVariable name="jserca" dimension="none" value="kserca*c">

        <DerivedVariable name="jleak" dimension="none" value="pleak*(cer-c)">

        <DerivedVariable name="jip3" dimension="none" value="oinf*(cer-c)">

        <DerivedVariable name="oinf" dimension="none" value="(c/(dact+c))*(ip3/(dip3+ip3))*(dinact/(dinact+c))">

        <DerivedVariable name="jer" dimension="none" value="jleak+jip3-jserca">


        <TimeDerivative variable="rIR" value="(rIRinf(v) - rIR)/tauIRr(v)"/>
        <TimeDerivative variable="rIR" value="(rIRinf(v) - rIR)/tauIRr(v)"/>
        <TimeDerivative variable="rIR" value="(rIRinf(v) - rIR)/tauIRr(v)"/>
        <TimeDerivative variable="rIR" value="(rIRinf(v) - rIR)/tauIRr(v)"/>
        <TimeDerivative variable="rIR" value="(rIRinf(v) - rIR)/tauIRr(v)"/>








        
        <StateVariable name="x" dimension="none" exposure="x"/>
        <OnStart>
            <StateAssignment variable="v" value="-60.0"/>
            <StateAssignment variable="c" value="0.10"/>
            <StateAssignment variable="n" value="0.01"/>
            <StateAssignment variable="cer" value="100"/>
            <StateAssignment variable="a" value="0.46"/>
            <StateAssignment variable="nIR" value="0.008"/>
            <StateAssignment variable="rIR" value="0.282"/>
        </OnStart>

        </Dynamics>

</ComponentType>


</Lems>